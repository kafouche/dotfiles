{
    "default_encoding": "UTF-8",
    "default_line_ending": "system",
    "draw_white_space": ["selection"],
    "rulers": [
        72,
        [80, "solid", 2]
    ],
    "show_encoding": true,
    "show_line_endings": true,
    "show_indentation": true,
    "show_syntax": true,
    "tab_size": 4,
    "theme": "Adaptive.sublime-theme",
    "translate_tabs_to_spaces": true,
    "ignored_packages":
    [
		"Vintage",
    ],
}
